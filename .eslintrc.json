{
  "extends": "airbnb",
  "env": {
      "es6": true,
      "browser": true,
      "node": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
      "ecmaFeatures": {
          "jsx": true
      }
  },
  "rules": {
      "react/jsx-filename-extension": 0,
      "import/prefer-default-export": 0,
      "react/forbid-prop-types": 0,
      "react/jsx-one-expression-per-line": 0,
      "implicit-arrow-linebreak": ["error", "beside"]
  }
}